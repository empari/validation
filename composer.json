{
    "name": "empari/validation",
    "description": "The Empari Validation package.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Adriano Santos",
            "email": "adrianodrix@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
